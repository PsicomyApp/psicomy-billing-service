{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BillingDb": "Host=localhost;Port=5432;Database=psicomy_billing;Username=postgres;Password=postgres",
    "DefaultConnection": "Host=localhost;Port=5432;Database=psicomy_billing;Username=postgres;Password=postgres"
  },
  "Stripe": {
    "SecretKey": "",
    "PublishableKey": "",
    "WebhookSecret": ""
  },
  "Jwt": {
    "Key": "",
    "Issuer": "psicomy-identity",
    "Audience": "psicomy-clients"
  },
  "Storage": {
    "Endpoint": "http://localhost:9000",
    "AccessKey": "",
    "SecretKey": "",
    "BucketName": "psicomy-billing",
    "Region": "us-east-1",
    "UseSSL": false
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/psicomy-billing-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": ["FromLogContext"]
  },
  "OpenTelemetry": {
    "ServiceName": "psicomy-billing-service",
    "ServiceVersion": "1.0.0",
    "OtlpEndpoint": "http://localhost:4317"
  }
}
